module bf_test;

import bf_parser;

bool checkNoRepeat (const RepeatedToken[] src) {
	BFTokenEnum last = src[0].token;
	foreach(rt;src[1 .. $]) {
		if (rt.token == last) {
			return false;
		} else {
			last = rt.token;
		}
	}
	
	return true;
}

